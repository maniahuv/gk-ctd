PROGRAM  NEWEXAMPLE;
VAR  N: INTEGER;
     RES: INTEGER;

FUNCTION SQUARE(V: INTEGER): INTEGER;
BEGIN
  SQUARE := V * V;
END;

(* Thủ tục in kết quả kèm xuống dòng *)
PROCEDURE DISPLAY(X: INTEGER);
BEGIN
  CALL WRITEI(X);
  CALL WRITELN;
END;

BEGIN
  FOR N := 1 TO 3 DO
    BEGIN
      RES := SQUARE(N);
      CALL DISPLAY(RES);
    END;
END.